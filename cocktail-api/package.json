      "@types/uuid": "^7.0.0",
      "bcrypt-nodejs": "0.0.3",
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "mongoose": "^5.8.9",
      "nodemon": "^2.0.0",
      "uuid": "^7.0.0"
   },
   "devDependencies": {
      "@typescript-eslint/eslint-plugin": "2.34.0",
      "@typescript-eslint/parser": "2.34.0",
      "concurrently": "5.2.0",
      "standardx": "5.0.0",
      "ts-node": "8.10.1",
